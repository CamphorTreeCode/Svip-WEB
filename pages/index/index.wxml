      <view class="leftText lf">{{notice1[0].shoptypename}}</view>
      <view class="imgsan ri">
        <image src="/img/sanjiaxin.png"></image>
      </view>
      <view class="rigngtext ri"  bindtap='jingshui'>更多</view>
    </view>

    <!--商品展示列表  start-->
