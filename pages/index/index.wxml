            <view class="yhjTextt yhjtxt">优惠劵</view>
            <view class="yhjTextt">{{item.discountMoney}}元</view>
          </view>
        </view>
        <view class="shoptitel">{{item.title}}</view>
        <view class="shoppriceText">
          <view class="shoppirce lf">
            ¥{{item.shopMoneydis}}
          </view>劵后
        </view>
      </view>

    </view>
